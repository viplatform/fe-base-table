{
  "name": "@viplatform/fe-base-table",
  "version": "1.0.6",
  "description": "Mui Tanstack React Table",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@viplatform"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "husky",
    "lint": "eslint ./src --ignore-pattern 'dist/**'",
    "lint:fix": "eslint ./src --fix --ignore-pattern 'dist/**'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "release": "standard-version"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viplatform/fe-base-table.git"
  },
  "author": "Bikash Das",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viplatform/fe-base-table/issues"
  },
  "homepage": "https://github.com/viplatform/fe-base-table#readme",
  "dependencies": {
    "@tanstack/react-table": "^8.20.5",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "material-react-table": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@eslint/js": "^9.7.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@mui/x-date-pickers": "^7.18.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "husky": "^9.1.1",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "react": "^18.2.1",
    "react-dom": "^18.2.0",
    "rollup": "^4.44.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.17.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/x-date-pickers": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "overrides": {
    "eslint": "^9.7.0"
  },
  "eslintIgnore": [
    "dist/**",
    "node_modules/**"
