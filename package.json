{
  "name": "mui-table",
  "version": "1.0.7",
  "description": "A reusable table component for React applications built with Material-UI (MUI) and TanStack Table.",
  "author": "Bikash Das <bikashdas0108@gmail.com> (https://github.com/bikashdas0108)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viplatform/mui-table/issues"
  },
  "homepage": "https://github.com/viplatform/mui-table#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/mui-table.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mui-table.es.js",
      "default": "./dist/mui-table.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viplatform/mui-table.git"
  },
  "keywords": [
    "react",
    "typescript",
    "mui",
    "javascript",
    "material",
    "table",
    "tanstack",
    "data-table"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "tsc --noEmit && eslint",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "release": "standard-version",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "prepublishOnly": "yarn build"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn build"
    },
    "skip": {
      "changelog": true
    }
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0",
    "@types/react": "^18.0.0 || ^19.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@tanstack/react-table": "^8.20.5",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "material-react-table": "^3.0.1",
    "@mui/icons-material": "^5.0.0",
    "@mui/x-date-pickers": "^7.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@eslint/js": "^9.7.0",
    "@mui/material": "^6.1.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "husky": "^9.1.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "react": "^18.2.1",
    "react-dom": "^18.2.0",
    "rollup": "^4.44.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass-embedded": "^1.89.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.6",
    "terser": "^5.39.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.4.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^1.6.0"
  }
}
